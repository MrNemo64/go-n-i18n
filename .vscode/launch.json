  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "./cmd/i18n/main.go",
      "args": [
        "-default-language",
        "en-EN",
        "-messages",
        "${cwd}/example/lang",
        "-out-file",
        "${cwd}/example/lang/generated_lang.go",
        "-out-package",
        "lang"
      ]
    }
  ]
}
